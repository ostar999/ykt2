<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="#5a6572"/>
            <size
                android:height="10dp"
                android:width="10dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="#5a6572"/>
            <size
                android:height="10dp"
                android:width="10dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="#959698"/>
            <size
                android:height="10dp"
                android:width="10dp"/>
        </shape>
    </item>
</selector>
