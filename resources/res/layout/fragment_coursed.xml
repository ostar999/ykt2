<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="?attr/app_bg"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.psychiatrygarden.widget.MyExpandableListView
        android:id="@+id/elv_tiku_question"
        android:background="?attr/app_bg"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:scrollbars="none"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:listSelector="@color/transparent"
        android:groupIndicator="@null"
        android:childDivider="?attr/line"
        android:divider="?attr/line"
        android:dividerHeight="0.5dp"
        android:footerDividersEnabled="true"
        android:elevation="0dp"/>
    <include
        android:visibility="gone"
        layout="@layout/adapter_empty_top_view"/>
    <LinearLayout
        android:id="@+id/lineimg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="150dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="?attr/ic_empty_data_normal"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
