                        android:textColor="?attr/tertiary_text_color"
                        android:gravity="right"
                        android:id="@+id/tv_job"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toLeftOf="@+id/iv_job"
                        android:layout_centerVertical="true"/>
                    <ImageView
                        android:id="@+id/iv_job"
                        android:background="?attr/backnewimg"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:contentDescription="@null"/>
                </RelativeLayout>
            </LinearLayout>
            <View style="@style/line_style"/>
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:id="@+id/rl_exams_work_time"
                android:padding="17dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="16sp"
                    android:textColor="?attr/first_text_color"
                    android:id="@+id/tv_exams_work_time1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="工作时间"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <TextView
                        android:textSize="16sp"
                        android:textColor="?attr/tertiary_text_color"
                        android:gravity="right"
                        android:id="@+id/tv_exams_work_time"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toLeftOf="@+id/iv_arrow13"
                        android:layout_centerVertical="true"/>
                    <ImageView
                        android:id="@+id/iv_arrow13"
                        android:background="?attr/backnewimg"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:contentDescription="@null"/>
                </RelativeLayout>
            </LinearLayout>
            <View style="@style/line_style"/>
        </LinearLayout>
        <View
            android:visibility="gone"
            android:layout_marginTop="20dp"
            style="@style/line_style"/>
        <View
            android:visibility="gone"
            android:layout_marginTop="20dp"
            style="@style/line_style"/>
        <RelativeLayout
            android:id="@+id/rl_modify_pwd"
            android:background="@color/white"
            android:padding="17dp"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
