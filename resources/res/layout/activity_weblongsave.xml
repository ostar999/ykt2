<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@+id/ly_view"
    android:background="?attr/bg_backgroud"
    android:fitsSystemWindows="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/nav_tabbar"
        android:background="?attr/new_bg_one_color"
        android:layout_width="match_parent"
        android:layout_height="48dp">
        <com.psychiatrygarden.widget.AlphaImageView
            android:id="@+id/iv_back"
            android:paddingLeft="17dp"
            android:paddingTop="5dp"
            android:paddingRight="22dp"
            android:paddingBottom="5dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:src="?attr/ic_black_back"
            android:layout_centerVertical="true"/>
        <TextView
            android:textSize="16sp"
            android:textColor="?attr/first_txt_color"
            android:ellipsize="end"
            android:gravity="center"
            android:id="@+id/title"
            android:paddingLeft="@dimen/dp_45"
            android:paddingRight="@dimen/dp_45"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="标题"
            android:maxLines="1"
            android:layout_centerInParent="true"
            android:paddingHorizontal="@dimen/dp_45"/>
    </RelativeLayout>
    <com.psychiatrygarden.widget.CustomEmptyView
        android:id="@+id/empty_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <android.webkit.WebView
        android:id="@+id/webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</LinearLayout>
