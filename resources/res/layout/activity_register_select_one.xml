<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_register_select_one"
    android:background="?attr/app_bg"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/activity_common_seach"/>
    <View
        android:id="@+id/view_line"
        android:background="?attr/gray_line_126"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@+id/rel_seach"/>
    <ListView
        android:id="@+id/mRegister_comList"
        android:background="@color/transparent"
        android:scrollbars="none"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="?attr/line"
        android:dividerHeight="0.7dp"
        android:layout_below="@+id/view_line"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"/>
    <LinearLayout
        android:gravity="center"
        android:orientation="vertical"
        android:id="@+id/ly_empty_view"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ImageView
            android:id="@+id/img_empty"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:src="?attr/ic_empty_data_normal"/>
        <TextView
            android:textSize="14sp"
            android:textColor="?attr/forth_txt_color"
            android:id="@+id/tv_empty"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="暂无数据"/>
    </LinearLayout>
</RelativeLayout>
