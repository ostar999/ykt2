<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="?attr/new_bg_two_color"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:background="?attr/new_bg_two_color"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44">
        <ImageView
            android:textSize="14sp"
            android:textColor="?attr/app_title_color"
            android:ellipsize="end"
            android:gravity="center"
            android:id="@+id/line_filtrate"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:src="?attr/index_change_img_personal"
            android:maxLines="1"
            android:contentDescription="@null"
            android:layout_marginStart="5dp"
            app:layout_constraintStart_toStartOf="parent"/>
        <TextView
            android:textSize="12sp"
            android:textColor="?attr/stolor"
            android:gravity="center"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:layout_constraintStart_toStartOf="parent"/>
        <LinearLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:id="@+id/linechange"
            android:padding="@dimen/dp_4"
            android:visibility="visible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <LinearLayout
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:id="@+id/ly_question"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="?attr/first_txt_color"
                    android:gravity="center"
                    android:id="@+id/tvQuestion"
                    android:background="?attr/app_bg"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dp_22"
                    android:text="@string/str_question_bank"
                    android:layout_weight="1"/>
                <ImageView
                    android:id="@+id/img_question"
                    android:visibility="gone"
                    android:layout_width="@dimen/dp_17"
                    android:layout_height="@dimen/dp_7"
                    android:layout_marginTop="@dimen/dp_5"
                    android:src="?attr/ic_comment_group_tag"
                    android:layout_below="@+id/tvQuestion"
                    android:layout_alignStart="@+id/tvQuestion"
                    android:layout_alignEnd="@+id/tvQuestion"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:id="@+id/ly_set_list"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_24">
                <TextView
                    android:textSize="@dimen/sp_14"
                    android:textColor="@drawable/tiku_top_textcolor"
                    android:gravity="center"
                    android:id="@+id/tvSetList"
                    android:background="?attr/app_bg"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dp_22"
                    android:text="@string/str_question_sheet"
                    android:layout_weight="1"/>
                <ImageView
                    android:id="@+id/img_set_list"
                    android:visibility="invisible"
                    android:layout_width="@dimen/dp_17"
                    android:layout_height="@dimen/dp_7"
                    android:layout_marginTop="@dimen/dp_5"
                    android:src="?attr/ic_comment_group_tag"
                    android:layout_below="@+id/tvSetList"
                    android:layout_alignStart="@+id/tvSetList"
                    android:layout_alignEnd="@+id/tvSetList"/>
            </LinearLayout>
        </LinearLayout>
        <TextView
            android:textSize="16sp"
            android:textColor="?attr/first_txt_color"
            android:id="@+id/title"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <TextView
            android:textSize="14sp"
            android:textColor="?attr/app_title_color"
            android:gravity="center"
            android:id="@+id/customimg"
            android:paddingLeft="@dimen/dp_5"
            android:paddingRight="@dimen/dp_5"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:contentDescription="@null"
            android:layout_marginEnd="@dimen/dp_11"
            android:paddingHorizontal="@dimen/dp_5"
            app:drawableRightCompat="?attr/customimg"
            app:layout_constraintEnd_toStartOf="@+id/tv_search"/>
        <TextView
            android:textSize="14sp"
            android:textColor="?attr/first_txt_color"
            android:gravity="center"
            android:id="@+id/tv_search"
            android:paddingLeft="@dimen/dp_5"
            android:paddingRight="@dimen/dp_5"
            android:visibility="visible"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:contentDescription="@null"
            android:layout_marginEnd="@dimen/dp_11"
            android:paddingHorizontal="@dimen/dp_5"
            app:drawableRightCompat="?attr/index_search_img"
            app:layout_constraintEnd_toStartOf="@+id/tv_woyaoshangchuan"/>
        <TextView
            android:textSize="14sp"
            android:textColor="?attr/app_title_color"
            android:gravity="center"
            android:id="@+id/tv_woyaoshangchuan"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/str_i_upload"
            android:contentDescription="@null"
            android:paddingEnd="10dp"
            android:layout_marginEnd="5dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <View
            android:id="@+id/question_mask"
            android:background="@color/dialog_transparent_bg"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <FrameLayout
        android:id="@+id/questioncom"
        android:background="?attr/new_bg_two_color"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/dp_44"/>
    <FrameLayout
        android:layout_gravity="bottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_gravity="end|bottom"
            android:id="@+id/iv_start_combine_question_shortcut"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="?attr/ic_start_combine_question_shortcut"/>
        <ImageView
            android:layout_gravity="bottom|center_horizontal"
            android:id="@+id/iv_start_combine_question"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="?attr/ic_start_combine_question"/>
    </FrameLayout>
</FrameLayout>
